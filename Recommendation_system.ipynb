      "source": [
        "def get_error(A, Users, Items, R):\n",
        "        return np.sum((R * (A - np.dot(Users, Items))) ** 2) / np.sum(R)\n",
        "\n",
        "def _ALS(self):\n",
        "\n",
        "    loss_list = np.array([])\n",
        "    n_factors = 2; n, m = self.U.shape\n",
        "    Users = 5 * np.random.rand(n, n_factors)\n",
        "    Items = 5 * np.random.rand(n_factors, m)\n",
        "\n",
        "    for iter in range(self.num_iter):\n",
        "        for i, Ri in enumerate(self.U):\n",
        "            Users[i] = np.linalg.solve(np.dot(Items, np.dot(np.diag(Ri), Items.T)) + self.learn_rt * np.eye(n_factors),\n",
        "                                       np.dot(Items, np.dot(np.diag(Ri), A[i].T))).T\n",
        "        \n",
        "        for j, Rj in enumerate(self.VT):\n",
        "            Items[:,j] = np.linalg.solve(np.dot(Users.T, np.dot(np.diag(Rj), Users)) + self.learn_rt * np.eye(n_factors),\n",
        "                                     np.dot(Users.T, np.dot(np.diag(Rj), A[:, j])))\n",
        "        \n",
        "        loss_list.append(get_error(self.U, Users, Items, self.VT))\n",
        "\n",
        "    return loss_list\n",
        "MatrixFactor._ALS = _ALS"
      ]
    },
